package overload;

import javax.sound.midi.Soundbank;

public class Smurf {

	public String name;
	public String color;
	
	public Smurf() {
		
		name = "일반 스머프";
		color = "파란색";
		
	}
	public Smurf(String name, String color) {
		
		this.name = name;
		this.color = color;
		System.out.println("지금부터 이 스머프의 이름은" + name + "입니다.");
		
	}
	
	public void call() {
		System.out.println("일반스머프 이리와");
		
	}
	public void call(String name) {
		System.out.println(name + "스머프 이리와");
	}
}
